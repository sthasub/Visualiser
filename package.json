{
  "name": "visualiser",
  "version": "1.0.0",
  "description": "Data Visualisation Application",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'visualiser-frontend/*'\" \"npm run visualiser-frontend\"",
    "visualiser-frontend": "cd visualiser-frontend && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd visualiser-frontend && npm install",
    "build": "cd visualiser-frontend && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "mongoose": "^5.3.16",
    "if-env": "^1.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SthaSub/Visualiser.git"
  },
  "keywords": [
    "N/A"
  ],
  "author": "Subhash Shrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SthaSub/Visualiser/issues"
  },
  "homepage": "https://github.com/SthaSub/Visualiser#readme"
}
